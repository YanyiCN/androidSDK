<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="397dp"
    android:layout_height="279dp"
    android:background="@drawable/jy_pc_bg" >
  
  <ImageButton
      android:id="@+id/btnPri"
      android:layout_width="58dp"
      android:layout_height="11dp"
      android:layout_alignTop="@+id/btnUser"
      android:layout_marginLeft="13dp"
      android:layout_toRightOf="@+id/btnUser"
      android:background="#00000000"
      android:scaleType="fitXY"
      android:src="@drawable/jy_pc_btn_pri" />

  <ImageButton
      android:id="@+id/btnUser"
      android:layout_width="37dp"
      android:layout_height="11dp"
      android:layout_alignParentLeft="true"
      android:layout_alignParentTop="true"
      android:layout_marginLeft="176dp"
      android:layout_marginTop="68dp"
      android:background="#00000000"
      android:scaleType="fitXY"
      android:src="@drawable/jy_pc_btn_user" />

  <ImageButton
      android:id="@+id/btnOk"
      android:layout_width="76dp"
      android:layout_height="26dp"
      android:layout_alignParentBottom="true"
      android:layout_centerHorizontal="true"
      android:layout_marginBottom="18dp"
      android:background="#00000000"
      android:scaleType="fitXY"
      android:src="@drawable/jy_pc_btn_ok" />

</RelativeLayout>